<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="@android:style/Theme"></style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="FM91CustomTheme" parent="@android:style/Theme.NoTitleBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">#00000000</item>
        <item name="android:colorBackground">#0000FF</item>
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="Theme.Default" parent="@android:style/Theme"></style>

    <style name="Theme.NoTitle" parent="@android:style/Theme.NoTitleBar">
        <item name="android:textColor">#000000</item>
        <item name="android:background">#000000</item>
    </style>

    <style name="Theme.FullScreen" parent="@android:style/Theme.NoTitleBar.Fullscreen"></style>

</resources>